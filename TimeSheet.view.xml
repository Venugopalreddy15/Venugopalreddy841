<mvc:View controllerName="ProjectTime.ZProjectTime.controller.TimeSheet" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:viz="sap.viz.ui5.controls" xmlns:layout="sap.ui.layout" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
	xmlns:microchart="sap.suite.ui.microchart" xmlns:viz.data="sap.viz.ui5.data" xmlns:core="sap.ui.core">
	<Page backgroundDesign="Transparent" class="sapUiSizeCompact sapUiContentPadding" showNavButton="false">
		<!--title="{i18n>ProjectTime} {oTimeshetModel>/StartDateMonth}-{oTimeshetModel>/EndDateMonth} ({i18n>Week} - {oTimeshetModel>/WeekNumber})"-->
		<Panel>
			<!--				<Text   text="Alert:  Atlas Project Time sheet app will be moved to a new Time-Card system.-->
			<!--Starting XXXXXXX, you need to access https://XXXXXX.com to access the new time card entry." class="redText largeText centerText"/>-->
		
			<Label text="Alert :" class="redText"/>
		
			<Label 
				text=" Time tracking is moving from ATLAS to TRIDENT Timecard from 1-Oct-2023. Look out for emails from Trident_global@dsi.com to learn more about TRIDENT Timecard." class="blueText" /> 
				<Label text="Details of your TRIDENT Timecard access will be coming late September.
				ATLAS time tracking will not be available after 13-Oct-2023." class="yellowText"/>
					<ToolbarSpacer/>
				<Label text="Please submit your September timecards by this date." class="whiteText"/>
			
		</Panel>
		<content>
			<!-- icon="{path: 'oUserModel>/Name', formatter: '.generateAvatar'}"-->
			<ObjectHeader visible="{= !${device>/system/phone} }" icon="{path: 'oUserModel>/Name', formatter: '.generateAvatar'}"
				number="{oTimeshetModel>/tableColumns/9/Total} Hrs" iconDensityAware="false" iconAlt="{oUserModel>/Name}" responsive="true"
				title="{oUserModel>/Name}" condensed="true" intro="{oUserModel>/PositionTitle}{oUserModel>/Department}"
				numberState="{= ${oTimeshetModel>/tableColumns/9/Total} >= 37.5 ? 'Success' : 'Error' }">
				<ObjectAttribute text="{oUserModel>/Txt} - CC#{oUserModel>/CostCenter}" title=""/>
				<!--<ObjectAttribute text="{oUserModel>/CostCenter}" title=""/>-->
				<ObjectAttribute text="Daiichi Sankyo Inc" title=""/>
				<statuses>
					<ProgressIndicator id="idProgressIndicator" width="100%" class="sapUiSmallMarginBottom"
						percentValue="{ path: 'oTimeshetModel>/tableColumns/9/Total', formatter: '.formatter.FloatProgressIndicator' }"
						displayValue="{i18n>SavedHour}{oTimeshetModel>/tableColumns/9/Total}/{i18n>TargetPerWeek}" showValue="true"
						state="{= ${oTimeshetModel>/tableColumns/9/Total} >= 37.5 ? 'Success' : 'Error' }"/>
				</statuses>
			</ObjectHeader>
			<Table id="dtimesheetTable" visible="{= !${device>/system/phone} }" inset="false" items="{ path: 'oTimeshetModel>/tableItems' }"
