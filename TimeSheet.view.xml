					<OverflowToolbar>
						<content>
							<Button icon="sap-icon://add" type="Emphasized"
								visible="{= ${oTimeshetModel>/tableItems/0/Lock} === 'X'|| ${oTimeshetModel>/user} === 'X' ? false : true }" tooltip="Add"
								press="onProjectAddPress"/>
