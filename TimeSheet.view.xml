								icon="{= ${oTimeshetModel>/VacationNotes} === '' ?'sap-icon://notes':'sap-icon://notification-2'}"
								type="{= ${oTimeshetModel>/VacationNotes} === '' ?'Accept':'Emphasized'}" tooltip="{i18n>WeeklyNotes}" press="onMainNotePress">
								<customData>
									<core:CustomData key="MainNote" value="{oTimeshetModel>/VacationNotes}"/>
								</customData>
							</Button>
							<ToolbarSpacer/>
						</content>
