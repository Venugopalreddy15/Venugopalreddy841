								visible="{= ${oTimeshetModel>Column} === ${oTimeshetModel>/tableColumns/4/Column}}" type="Transparent">
								<customData>
									<core:CustomData key="Day03" value="{oNotesModel>/Day03}"/>
								</customData>
							</Button>
