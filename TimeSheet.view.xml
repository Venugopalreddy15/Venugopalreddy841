								visible="{= ${oTimeshetModel>Column} === ${oTimeshetModel>/tableColumns/6/Column}}" type="Transparent">
								<customData>
									<core:CustomData key="Day05" value="{oNotesModel>/Day05}"/>
								</customData>
							</Button>
							<Button text="{oTimeshetModel>Column}" press="onMainNotePress" enabled="{= ${oTimeshetModel>/firstTimeUser/Saturday} ?false:true}"
