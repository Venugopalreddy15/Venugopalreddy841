								<customData>
									<core:CustomData key="Day06" value="{oNotesModel>/Day06}"/>
								</customData>
							</Button>
							<Button text="{oTimeshetModel>Column}" press="onMainNotePress" enabled="{= ${oTimeshetModel>/firstTimeUser/Sunday} ?false:true}"
