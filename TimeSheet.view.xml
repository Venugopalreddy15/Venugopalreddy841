							<Button text="{oTimeshetModel>Column}" press="onMainNotePress" icon="{= ${oNotesModel>/Day01} !== ''?'sap-icon://notification-2':''}"
								enabled="{= ${oTimeshetModel>/firstTimeUser/Monday} ?false:true}"
								visible="{= ${oTimeshetModel>Column} === ${oTimeshetModel>/tableColumns/2/Column}}" type="Transparent">
								<customData>
									<core:CustomData key="Day01" value="{oNotesModel>/Day01}"/>
								</customData>
							</Button>
							<Button text="{oTimeshetModel>Column}" press="onMainNotePress" enabled="{= ${oTimeshetModel>/firstTimeUser/Tuesday} ?false:true}"
								icon="{= ${oNotesModel>/Day02} !== ''?'sap-icon://notification-2':''}"
								visible="{= ${oTimeshetModel>Column} === ${oTimeshetModel>/tableColumns/3/Column}}" type="Transparent">
								<customData>
									<core:CustomData key="Day02" value="{oNotesModel>/Day02}"/>
								</customData>
							</Button>
							<Button text="{oTimeshetModel>Column}" press="onMainNotePress" enabled="{= ${oTimeshetModel>/firstTimeUser/Wednesday} ?false:true}"
