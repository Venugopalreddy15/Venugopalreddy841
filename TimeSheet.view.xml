								<customData>
									<core:CustomData key="Day04" value="{oNotesModel>/Day04}"/>
								</customData>
							</Button>
							<Button text="{oTimeshetModel>Column}" press="onMainNotePress" enabled="{= ${oTimeshetModel>/firstTimeUser/Friday} ?false:true}"
